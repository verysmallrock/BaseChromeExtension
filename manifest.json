{
  "name": "Stupid Popup Blocker",
  "version": "1.0",
  "description": "No more upsetness",
  "manifest_version": 2,
  "permissions": [
      "activeTab"
   ],
  "background": {
      "scripts": ["background.js"],
      "persistent": true
   },
   "content_scripts": [
	   {
	     "matches": ["<all_urls>"],
	     "js": ["content.js"]
	   }
 	]
}
